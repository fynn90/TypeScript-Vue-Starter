{
  "name": "TypeScript-Vue-Starter",
  "version": "1.0.0",
  "description": "Typescript+Vue 项目案例",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "testMocha": "cross-env NODE_ENV=test nyc mocha-webpack --webpack-config webpack.config.js --require test/setup.js test/**/*.spec.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --mode development --inline --hot --color",
    "build": "webpack --mode development --color"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:fynn90/TypeScript-Vue-Starter.git"
  },
  "keywords": [
    "Vue",
    "Typescript"
  ],
  "author": "fynn",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "vue": "^2.5.15",
    "vue-class-component": "^6.2.0",
    "vue-lazyload": "^1.2.2",
    "vue-property-decorator": "^6.0.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-class": "^0.3.0",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@types/jest": "^22.2.0",
    "@vue/test-utils": "^1.0.0-beta.12",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.10",
    "expect": "^22.4.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.0.6",
    "jest": "^23.0.0-alpha.0",
    "jest-serializer-vue": "^1.0.0",
    "jsdom": "^11.6.2",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.0.4",
    "mocha-webpack": "^1.0.1",
    "nyc": "^11.6.0",
    "postcss": "^6.0.19",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.1",
    "postcss-pxtorem": "^4.0.1",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.20.2",
    "ts-jest": "^22.4.1",
    "ts-loader": "^4.0.1",
    "typescript": "^2.7.2",
    "url-loader": "^1.0.1",
    "vue-jest": "^2.1.1",
    "vue-loader": "^14.2.1",
    "vue-style-loader": "^4.0.2",
    "vue-template-compiler": "^2.5.15",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.11",
    "webpack-dev-server": "^3.1.1",
    "webpack-node-externals": "^1.6.0"
  },
  "nyc": {
    "include": [
      "src/**/*.(js|vue)"
    ],
    "instrument": false,
    "sourceMap": false
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "tsx",
      "vue"
    ],
    "testRegex": "(/__tests__/.*|\\.(test))\\.(ts|tsx|js)$",
    "transform": {
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
      "^.+\\.tsx?$": "ts-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ]
  }
}
